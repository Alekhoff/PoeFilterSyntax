{
	"$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
	"name": "Filter",
	"patterns": [{
			"include": "#operator"
		},
		{
			"include": "#number"
		},
		{
			"include": "#keywords"
		},
		{
			"include": "#attribute"
		},
		{
			"include": "#strings"
		},
		{
			"include": "#comment-line"
		}
	],
	"repository": {
		"operator": {
			"patterns": [{
				"name": "keyword",
				"match": "(=|<|>)"
			}]
		},
		"number": {
			"patterns": [{
				"name": "entity.name.function",
				"match": "(1|2|3|4|5|6|7|8|9|0|Rare|Unique|Normal|Magic|RGB|Maps)"
			}]
		},
		"keywords": {
			"patterns": [{
				"name": "keyword",
				"match": "\\b(Show|Hide)\\b"
			}]
		},
		"attribute": {
			"patterns": [{
				"name": "variable",
				"match": "\\b(LinkedSockets|SetTextColor|SetBackgroundColor|SetBorderColor|SetFontSize|PlayAlertSound|BaseType|Class|Rarity|Prophecy|ItemLevel|DropLevel|Sockets|SocketGroup|Width|Height|StackSize|Quality|Corrupted|GemLevel|ShapedMap|ElderMap|Identified|HasExplicitMod|AnyEnchantment|HasEnchantment|ShaperItem|FracturedItem|SynthesisedItem|MinimapIcon|PlayEffect|ElderItem|DisableDropSound|MapTier)\\b"
			}]
		},
		"strings": {
			"name": "string",
			"begin": "\"",
			"end": "\"",
			"patterns": [{
				"name": "string",
				"match": "\\\\"
			}]
		},
		"comment-line": {
			"name": "comment",
			"begin": "#",
			"end": "\n",
			"patterns": [{
				"name": "comment",
				"match": "\\\\"
			}]
		}
	},
	"scopeName": "source.filter"
}
